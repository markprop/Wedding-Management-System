  "repository": {
    "type": "git",
    "url": "https://github.com/markprop/Wedding-Management-System.git"
  },
  "homepage": "https://github.com/markprop/Wedding-Management-System#readme",
  "bugs": {
    "url": "https://github.com/markprop/Wedding-Management-System/issues"
  }
}
